{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:48866",
      "sslPort": 44373
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "api/tweet",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DATABASE_NAME": "TweetsDatabase",
        "TWEET_COLLECTION_NAME": "Tweets"
      }
    },
    "TweetMicroservice": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "api/tweet",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DATABASE_NAME": "TweetsDatabase",
        "TWEET_COLLECTION_NAME": "Tweets"
      },
      "applicationUrl": "https://localhost:5002;http://localhost:5000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/api/tweet",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}